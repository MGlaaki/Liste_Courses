<%= form_for @partage do |f| %>

  <div><%= f.label "Liste à partager" %></div>
  <div><%= f.collection_select :liste_id, @listes, :id, :nom_liste, include_blank: false%></div>

  <div><%= f.label "Avec" %>
  <div><%= f.text_field :destinataire_name %></div>

  <div><%= f.submit %><div>

<% end %>

<p id=alert><%=alert%></p>

<div><%=link_to "Retour aux partages", partages_path %></div>
<div><%=link_to "Retour à l'accueil", root_path %></div>
